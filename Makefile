# This Makefile was generated by the ThirdCake
# https://github.com/grwlf/cake3

GUARD = .GUARD_$(1)_$(shell echo $($(1)) | md5sum | cut -d ' ' -f 1)
URFLAGS = 
.PHONY: all
all: App.exe Makefile
.PHONY: sql
sql: App.db Makefile
	sqlite3 App.db
Makefile: ./Cakegen
	./Cakegen > Makefile
App.db App.exe App.sql: stamp1
.INTERMEDIATE: stamp1
stamp1: ./App.urp ./src/static/Static.urp Makefile src/All1.ur src/All1.urs $(call GUARD,URFLAGS)
	urweb -dbms sqlite $(URFLAGS) App
	-rm -rf App.db
	sqlite3 App.db < App.sql
./src/static: Makefile
	mkdir -pv ./src/static
./src/static/Static.urp: ./src/Menu.js ./src/static Makefile $(call GUARD,HOME)
	urembed  -c `which gcc` -I $(HOME)/local/include/urweb -o ./src/static/Static.urp ./src/Menu.js
$(call GUARD,HOME):
	rm -f .GUARD_HOME_*
	touch $@
$(call GUARD,URFLAGS):
	rm -f .GUARD_URFLAGS_*
	touch $@

